def find_unique_elements(input_list):
    unique_list = list(set(input_list))
    return unique_list

# Test Cases
input1 = [3, 7, 3, 5, 2, 5, 9, 2]
input2 = [-1, 2, -1, 3, 2, -2]
input3 = [1000000, 999999, 1000000]

output1 = find_unique_elements(input1)
output2 = find_unique_elements(input2)
output3 = find_unique_elements(input3)

print("Output 1:", output1)
print("Output 2:", output2)
print("Output 3:", output3)
