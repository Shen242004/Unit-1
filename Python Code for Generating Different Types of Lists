def generate_lists(input_list):
    if len(input_list) <= 1:
        return input_list
    else:
        return sorted(input_list)

# Test Cases
test_cases = [[], [1], [7, 7, 7, 7], [-5, -1, -3, -2, -4]]

for case in test_cases:
    result = generate_lists(case)
    print(f"Input: {case}")
    print(f"Expected Output: {result}")
    print()
